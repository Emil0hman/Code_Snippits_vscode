{
    "name": "code-snippits",
    "displayName": "Code Snippits",
    "description": "Generate HTML, CSS and JavaScript snippits!",
    "version": "0.0.4",
    "publisher": "emil",
    "icon": "icon2.png",
    "repository": {
        "type": "git",
        "url": "https://github.com/Emil0hman/Code_Snippits_vscode"
    },
    "engines": {
        "vscode": "^1.29.0"
    },
    "categories": [
        "Snippets"
    ],
    "contributes": {
        "snippets": [
            {
                "language": "html",
                "path": "./snippets/snippets_html.json"
            },
            {
                "language": "javascript",
                "path": "./snippets/snippets_js.json"
            },
            {
                "language": "css",
                "path": "./snippets/snippets_css.json"
            },
            {
                "language": "typescript",
                "path": "./snippets/snippets_js.json"
            },
            {
                "language": "scss",
                "path": "./snippets/snippets_css.json"
            }
        ]
    }
}
